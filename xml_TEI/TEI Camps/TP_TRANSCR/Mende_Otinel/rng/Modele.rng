               </zeroOrMore>
               <optional>
                  <ref name="wit"/>
                  <zeroOrMore>
                     <choice>
                        <ref name="model.global.meta"/>
                        <ref name="model.milestoneLike"/>
                        <ref name="model.noteLike"/>
                        <ref name="gap"/>
                        <ref name="space"/>
                     </choice>
                  </zeroOrMore>
               </optional>
            </optional>
            <zeroOrMore>
               <choice>
                  <group>
                     <ref name="model.rdgLike"/>
                     <zeroOrMore>
                        <choice>
                           <ref name="model.global.meta"/>
                           <ref name="model.milestoneLike"/>
                           <ref name="model.noteLike"/>
                           <ref name="gap"/>
                           <ref name="space"/>
                        </choice>
                     </zeroOrMore>
                     <optional>
                        <ref name="wit"/>
                        <zeroOrMore>
                           <choice>
                              <ref name="model.global.meta"/>
                              <ref name="model.milestoneLike"/>
                              <ref name="model.noteLike"/>
                              <ref name="gap"/>
                              <ref name="space"/>
                           </choice>
                        </zeroOrMore>
                     </optional>
                  </group>
                  <group>
                     <ref name="rdgGrp"/>
                     <zeroOrMore>
                        <choice>
                           <ref name="model.global.meta"/>
