                           <ref name="model.noteLike"/>
                           <ref name="gap"/>
                           <ref name="space"/>
                        </choice>
                     </zeroOrMore>
                     <optional>
                        <ref name="wit"/>
                        <zeroOrMore>
                           <choice>
                              <ref name="model.global.meta"/>
